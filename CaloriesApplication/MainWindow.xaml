<Window x:Class="TeamProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button Background="AliceBlue" DockPanel.Dock="Top" Margin="10" Click="Button_Click">Click me</Button>
            <Button Background="Blue" Margin="10">Call</Button>
            <Button Content="Check Database" Click="CheckDatabaseButton_Click"/>
        </DockPanel>
    </Grid>
</Window>
